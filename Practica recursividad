#include <iostream>
using namespace std;

int fibonacci (int n);

int main(){
    int num;
    cout<<"Ingresar el numero termino de la Serie de Fibonacci: ";
    cin>>num;
    cout<<"El resultado es: "<<fibonacci(num);
    return 0;
}
int fibonacci (int n){
    if (n==0) return 0;
    if (n==1) return 1;
    return fibonacci(n-1)+fibonacci(n-2);
}
